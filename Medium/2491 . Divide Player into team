class Solution:
    def dividePlayers(self, skill: List[int]) -> int:
        skill.sort()
        i=0
        j=len(skill)-1
        target=skill[i]+skill[j]
        answer=0
        while(i<j):
            if(skill[i]+skill[j]==target):
                answer+=skill[i]*skill[j]
                i+=1
                j-=1
            else:
                return -1    
        return answer